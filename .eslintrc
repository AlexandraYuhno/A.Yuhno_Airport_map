{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "vue/no-unused-vars": "warn",
        "vue/no-unused-components": "warn",
        "no-console": "off", 
        "no-debugger": "off"
    }
}
